{
  "name": "cypress-quality-engineering-lab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "cy:open": "node scripts/cypress-cli.js open",
    "cy:run": "node scripts/cypress-cli.js run",
    "cy:verify": "node scripts/cypress-cli.js verify",
    "test:smoke": "node scripts/cypress-cli.js run --spec \"cypress/e2e/smoke/**/*.cy.js\"",
    "test:api": "node scripts/cypress-cli.js run --spec \"cypress/e2e/api/**/*.api.cy.js,cypress/e2e/api/**/*.contract.cy.js\"",
    "test:ui": "node scripts/cypress-cli.js run --spec \"cypress/e2e/ui/**/*.ui.cy.js\"",
    "test:regression": "node scripts/cypress-cli.js run --spec \"cypress/e2e/api/**/*.cy.js,cypress/e2e/ui/**/*.cy.js\"",
    "report:clean": "node cypress/scripts/clean-reports.js",
    "report:html": "node cypress/scripts/generate-html-reports.js",
    "report": "npm run report:clean && npm run cy:run && npm run report:html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "ajv": "^8.18.0",
    "cypress": "^15.10.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.4",
    "mochawesome-merge": "^5.1.1",
    "mochawesome-report-generator": "^6.3.2"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "diff": "^8.0.2"
  }
}
